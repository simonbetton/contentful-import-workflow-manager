{
  "name": "@simonbetton/contentful-import-workflow-manager",
  "description": "A simple tool to extract the contentful-cli export file into a more manageable workflow.",
  "version": "0.0.0",
  "author": "Simon Betton (https://github.com/simonbetton)",
  "bin": {
    "contentful-import-workflow-manager": "./bin/run",
    "ciwm": "./bin/run"
  },
  "bugs": "https://github.com/simonbetton/contentful-import-workflow-manager/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/fs-extra": "^9.0.12",
    "chalk": "^4.1.2",
    "conf": "^10.0.2",
    "contentful-cli": "^1.8.23",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "prettier": "^2.3.2",
    "slugify": "^1.6.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/prettier": "^2.3.2",
    "chai": "^4",
    "contentful-typescript-codegen": "^3.2.2",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/simonbetton/contentful-import-workflow-manager",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "contentful-import-workflow-manager",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "simonbetton/contentful-import-workflow-manager",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "contentful-typescript-codegen": "contentful-typescript-codegen --output @types/generated/contentful.d.ts"
  },
  "types": "lib/index.d.ts"
}
